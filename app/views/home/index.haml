#picker
  %ul
    - @bands.each_with_index do |band, ii|
      %li.band{data: {track_id: band.track_id, band_id: band.id}}
        %figure{style: "background-image: url(#{band.thumbnail.url})"}
          .icon
            %i.icon-play
          .veil
        .name
          %h5= band.name
          %h6= band.track_name
        .clear
  -#%button#pause PAUSE
  .clear
